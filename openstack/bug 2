        version = client.version()['ApiVersion']
        
        if compare_version(version, '1.1.7'):
            start_args[self.READ_ONLY] = self.properties[self.READ_ONLY]
        else:
            LOG.warn(_LW("Read-only is not supported for"
                         " API version < 1.17, it is ignored"))
                             

from distutils.version import StrictVersion                          
def compare_version(v1, v2):
    s1 = StrictVersion(v1)
    s2 = StrictVersion(v2)
    if s1 == s2:
        return 0
    elif s1 > s2:
        return -1
    else:
        return 1
