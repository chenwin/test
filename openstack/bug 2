        if self.properties[self.READ_ONLY]:
            if compare_version(client.version, '1.0.0'):
                start_args[self.READ_ONLY] = True
            else:
                LOG.warn(_LW("Read-only is not supported in"
                             " docker-py < 1.0.0, it is ignored"))
                             

from distutils.version import StrictVersion                          
def compare_version(v1, v2):
    s1 = StrictVersion(v1)
    s2 = StrictVersion(v2)
    if s1 == s2:
        return 0
    elif s1 > s2:
        return -1
    else:
        return 1
